<!DOCTYPE html>
<html>
<head>
  <title>SDS Donor Database</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= yield %>

<% if @current_user %>
<p><em>Currently logged in as <%= link_to @current_user.name, @current_user %>.  <%= link_to 'Logout', logout_path, :method => :post %>.</em></p>
<% end %>
</body>
</html>
